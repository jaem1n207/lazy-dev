    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install",
    "commit": "git-cz",
    "post": "node ./src/utils/post.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
