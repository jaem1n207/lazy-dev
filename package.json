{
  "name": "lazy-dev",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "lazy-dev",
  "version": "0.1.0",
  "author": "jaem1n207 <roy.jm.lee@gmail.com>",
  "bugs": {
    "url": "https://github.com/jaem1n207/lazy-dev/issues"
  },
  "homepage": "https://lazydev.gatsbyjs.io",
