{
  "name": "lazy-dev",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "lazy-dev",
  "version": "0.1.0",
  "author": "jaem1n207 <roy.jm.lee@gmail.com>",
  "bugs": {
    "url": "https://github.com/jaem1n207/lazy-dev/issues"
  },
  "homepage": "https://lazy-dev.netlify.app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaem1n207/lazy-dev.git"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "post": "node ./src/common/utils/post.cjs",
    "prepare": "husky install",
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "prettier:format": "prettier -c --write **/* --cache",
    "biome:format": "biome format --write .",
    "lint-fix:script": "biome check --apply ."
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "biome format --write",
      "biome check --apply ."
    ],
    "**/*.{css,md,json}": [
      "prettier --ignore-unknown --write"
    ]
  },
